	</xs:complexType>
	<xs:complexType name="ChangeTrackingType">
		<xs:complexContent>
			<xs:extension base="BaseType">
				<xs:sequence>
					<xs:element maxOccurs="unbounded" name="ChangeTrackingID" type="string_Stype"/>
