      </xs:complexContent>
   </xs:complexType>
   <xs:complexType name="RefRuleBoolType000">
      <xs:complexContent>
         <xs:extension base="ReturnBoolType">
            <xs:attribute ref="not"/>
            <xs:attribute name="itemName" type="xs:NCName" use="required">
